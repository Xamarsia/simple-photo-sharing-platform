{
    "name": "SPSP backend",
    "dockerComposeFile": "docker-compose.yml",
    "service": "api",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "customizations": {
        "vscode": {
            "settings": {},
            "extensions": [
                "esbenp.prettier-vscode",
                "vscjava.vscode-java-pack",
                "eamodio.gitlens",
                "inferrinizzard.prettier-sql-vscode",
                "redhat.vscode-xml"
            ]
        }
    },
    // need for git
    "postCreateCommand": "git config --global --add safe.directory /workspaces/${localWorkspaceFolderBasename} && mvn package"
}
